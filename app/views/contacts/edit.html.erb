<form action="/contacts/<%= @contact.id %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <p>first_name: <input type="text" name="title" value="<%= @contact.first_name %>"></p>
  <p>last_name: <input type="text" name="ingredients" value="<%= @contact.last_name %>"></p>
  <p>phone_number: <input type="text" name="directions" value="<%= @contact.phone_number %>"></p>
  <p>email: <input type="text" name="email" value="<%= @contact.email %>"></p>
  <input type="submit" value="edit this contact">
</form>